{
    "name": "erikfig/direto-au-ponto-plugin-cakephp",
    "description": "Accounts plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Accounts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accounts\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
